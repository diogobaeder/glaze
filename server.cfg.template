[circus]
logoutput = %(logoutput)s
debug = True

[watcher:dwebworker]
cmd = .env/bin/uwsgi
    --http 0.0.0.0:%(server_port)s
    --wsgi-file %(project)s/wsgi.py
    --processes %(server_processes)s
send_hup = True
stop_signal = QUIT
working_dir = %(project_dir)s

[env:dwebworker]
PYTHONPATH = %(project_dir)s
DJANGO_SETTINGS_MODULE=%(django_settings_module)s
